<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <RecipeDef>
    <defName>Recipe_MakeCrateMaxim</defName>
    <label>make maxim machine gun crate</label>
    <description>make a maxim machine gun crate. you need this to build maxim turret.</description>
    <jobString>Cutting gun.</jobString>
    <workAmount>30000</workAmount>
    <workSkill>Crafting</workSkill>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <effectWorking>Smith</effectWorking>
    <unfinishedThingDef>UnfinishedGun</unfinishedThingDef>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Steel</li>
          </thingDefs>
        </filter>
        <count>75</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>ComponentIndustrial</li>
          </thingDefs>
        </filter>
        <count>3</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>Steel</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <AAA_Crate_Maxim>1</AAA_Crate_Maxim>
    </products>
    <recipeUsers>
      <li>FueledSmithy</li>
      <li>ElectricSmithy</li>
      <li>TableMachining</li>
    </recipeUsers>
    <researchPrerequisite>BlowbackOperation</researchPrerequisite>
  </RecipeDef>
